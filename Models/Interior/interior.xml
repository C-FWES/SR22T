	<!--IGNITION KEY -->
	<animation>
		<type>pick</type>
		<object-name>ignition_bg</object-name>
		<object-name>ignition_turn</object-name>
		<action>
			<name>get_key</name>
			<button>0</button>
			<binding>
				<condition>
					<equals>
						<property>/controls/engines/engine/magnetos</property>
						<value>0</value>
					</equals>
					<not>
						<property>/controls/engines/engine/starter</property>
					</not>
				</condition>
				<command>property-toggle</command>
				<property>/controls/engines/key</property>
			</binding>
		</action>
		<hovered>
			<binding>
				<condition>
					<equals>
						<property>/controls/engines/engine/magnetos</property>
						<value>0</value>
					</equals>
					<not>
						<property>/controls/engines/engine/starter</property>
					</not>
				</condition>
				<command>set-tooltip</command>
				<tooltip-id>get-key</tooltip-id>
				<label>Put key</label>
			</binding>
		</hovered>
	</animation>
	<animation>
		<type>select</type>
		<object-name>ignition_key</object-name>
		<object-name>start</object-name>
		<condition>
			<property>/controls/engines/key</property>
		</condition>
	</animation>
	<!-- Magneto switch -->
	<animation>
		<type>knob</type>
		<object-name>ignition_key</object-name>
		<visible>true</visible>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>/controls/engines/engine/magnetos</property>
				<factor>1</factor>
				<min>0</min>
				<max>3</max>
			</binding>
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>magneto-switch</tooltip-id>
				<label>Magnetos: %s</label>
				<property>controls/engines/engine/magnetos</property>
				<mapping>nasal</mapping>
				<script>
					var m = arg[0];
					if (m == 1) return 'RIGHT';
					if (m == 2) return 'LEFT';
					if (m == 3) return 'BOTH';
					return 'OFF';
				</script>
			</binding>
		</hovered>
	</animation>
	<!-- Starter -->
	<animation>
		<type>pick</type>
		<object-name>start</object-name>
		<visible>false</visible>
		<action>
			<name>starter</name>
			<button>0</button>
			<binding>
				<command>property-assign</command>
				<property>/controls/engines/engine/starter</property>
				<value>1</value>
			</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>/controls/engines/engine/starter</property>
					<value>0</value>
				</binding>
			</mod-up>
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>starter-switch</tooltip-id>
				<label>Engine Starter</label>
			</binding>
		</hovered>
	</animation>
	<!-- Key rotation animation -->
	<animation>
		<type>rotate</type>
		<object-name>ignition_key</object-name>
		<object-name>ignition_turn</object-name>
		<expression>
			<sum>
				<product>
					<!-- 1 if starter is false, 0 if starter is true -->
					<difference>
						<value>1</value>
						<property>/controls/engines/engine/starter-pos</property>
					</difference>
					<property>/controls/engines/engine/magnetos-pos</property>
					<value>30</value>
				</product>
				<product>
					<property>/controls/engines/engine/starter-pos</property>
					<value>120</value>
				</product>
			</sum>
		</expression>
		<center>
			<x-m>-1.98749</x-m>
			<y-m>-0.53428</y-m>
			<z-m>0.10659</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>


