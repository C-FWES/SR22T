		<gear-warn>
            <name>gear warning</name>
            <mode>looped</mode>
            <path>gear-hrn.wav</path>
            <condition>
				<or>
				<and>
					<equals>
						<property>/controls/lighting/gearwrn</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/controls/switches/test-transit-button</property>
						<value>1</value>
					</equals>
				</and>
				<and>
					<equals>
						<property>/controls/lighting/gearwrn</property>
						<value>1</value>
					</equals>
					<or>
						<less-than>
							<property>/controls/engines/engine[0]/throttle</property>
							<value>0.25</value>
						</less-than>
						<less-than>
							<property>/controls/engines/engine[1]/throttle</property>
							<value>0.25</value>
						</less-than>
						<equals>
							<property>/controls/flight/flaps</property>
							<value>1</value>
						</equals>
					</or>
					<or>
					  <less-than>
						<property>gear/gear[0]/position-norm</property>
						<value>0.95</value>
					  </less-than>
					  <less-than>
						<property>gear/gear[1]/position-norm</property>
						<value>0.95</value>
					  </less-than>
					  <less-than>
						<property>gear/gear[2]/position-norm</property>
						<value>0.95</value>
					  </less-than>
					</or>
				</and>
				</or>
            </condition>
			<position>
				<x>-2.0</x>
				<y>0.40</y>
				<z>1.40</z>
			</position>
			<reference-dist>2.0</reference-dist>
			<max-dist>5.0</max-dist>
		</gear-warn>
		
