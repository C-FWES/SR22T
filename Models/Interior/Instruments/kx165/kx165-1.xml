      <blue>  0.1 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>CommUse</object-name>
    <object-name>CommStby</object-name>
    <transparency>
      <alpha-prop>systems/electrical/outputs/comm</alpha-prop>
      <factor>0.07</factor>
