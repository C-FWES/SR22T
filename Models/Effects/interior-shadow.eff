<?xml version="1.0" encoding="utf-8"?>
<PropertyList>
    <name>interior-shadow</name>
    <inherits-from>Effects/model-interior</inherits-from>
    <parameters>
        <texture n="4">
            <type>cubemap</type>
            <images>
                <positive-x>Aircraft/SR22T/Models/Effects/interior/black/0004.png</positive-x>
                <!-- px (tail) rotate 90 deg from N or S to W -->
                <negative-x>Aircraft/SR22T/Models/Effects/interior/black/0002.png</negative-x>
                <!-- nx (nose) rotate 90 deg from N or S to E -->
                <positive-y>Aircraft/SR22T/Models/Effects/interior/black/0001.png</positive-y>
                <!-- py (starboard/right) from N do not rotate -->
                <negative-y>Aircraft/SR22T/Models/Effects/interior/black/0003.png</negative-y>
                <!-- ny (port/left) from N rotate 180 deg to S -->
                <positive-z>Aircraft/SR22T/Models/Effects/interior/black/0006.png</positive-z>
                <!-- pz (top) rotate 90 deg from N or S to W -->
                <negative-z>Aircraft/SR22T/Models/Effects/interior/black/0005.png</negative-z>
                <!-- nz (bottom) rotate 90 deg from N or S to W -->
            </images>
        </texture>
        <opacity-cube-center type="vec3d" n="0"> -1.06282 0.0 0.51736 </opacity-cube-center>
        <opacity-cube-scale type="vec3d" n="0"> 1 0.6 0.4</opacity-cube-scale>
        <opacity-cube-angle type="float">0.0</opacity-cube-angle>
    </parameters>
</PropertyList>
