  <!-- actions -->
  <animation>
    <type>pick</type>
    <object-name>CommVolume</object-name>
    <visible>true</visible>
    <action>
      <button>4</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-adjust</command>
        <property alias="../../../../params/comm-volume"/>
        <step>0.1</step>
        <min>0.0</min>
        <max>1.0</max>
        <wrap>false</wrap>
      </binding>
      <binding>
        <command>property-assign</command>
        <property alias="../../../../params/power-comm"/>
        <value>1</value>
        <condition>
          <greater-than>
            <property alias="../../../../../../params/comm-volume"/>
            <value>0.05</value>
          </greater-than>
        </condition>
      </binding>
    </action>
    <action>
      <button>3</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-adjust</command>
        <property alias="../../../../params/comm-volume"/>
        <step>-0.1</step>
        <min>0.0</min>
        <max>1.0</max>
        <wrap>false</wrap>
      </binding>
      <binding>
        <command>property-assign</command>
        <property alias="../../../../params/power-comm"/>
        <value>0</value>
        <condition>
          <less-than>
            <property alias="../../../../../../params/comm-volume"/>
            <value>0.05</value>
          </less-than>
        </condition>
      </binding>
    </action>
  </animation>

